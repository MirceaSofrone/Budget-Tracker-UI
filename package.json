    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.50.0",
    "eslint-plugin-sonarjs": "^0.21.0",
    "husky": "^8.0.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
